{
  "manifest_version": 2,
  "name": "Mark",
  "description": "A chrome extension that enables quick html modification on any web page.",
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "32": "assets/icon32.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    },
    "default_title": "Mark"
  },
  "permissions": [
    "activeTab"
  ]
}
